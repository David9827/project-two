{
  "manifest_version": 2,
  "name": "Auto Add and Submit Codes",
  "description": "Automatically add numbers to codes and submit them to the web",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://napthe.vn/app/100054/buy/786432"],
      "js": ["content.js"]
    }
  ]
}
